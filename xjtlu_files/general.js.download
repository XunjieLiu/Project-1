AccordionController = (function() {

    var accordion = function () {
        this.init();
    };

    accordion.prototype.init = function () {
        var setup = createDelegate( this, this.setupAccordion );
        $( '.accordion-section' ).each( function() { setup( this ) } );
    };

    accordion.prototype.setupAccordion = function( elem ) {
        $( elem ).find( '.content' ).hide();
        $( elem ).find( 'li' ).eq( 0 ).addClass( 'active' ).find( '.content' ).show();
        $( elem ).find( 'button' ).on( 'click ', this.accordionSectionToggle );
    };

    accordion.prototype.accordionSectionToggle = function( e ){
        var elem = $( e.target );
        if (elem.closest('li').hasClass('active')) {
            elem.closest('li').removeClass('active').find('.content').slideUp(200).attr('aria-hidden', true);
        } else {
            elem.closest('li').addClass('active').siblings('li').removeClass('active').find('.content').slideUp(200).attr('aria-hidden', true);
            elem.closest('li').find('.content').slideDown(200).attr('aria-hidden', false);
        }
    };

    return accordion;
})();

