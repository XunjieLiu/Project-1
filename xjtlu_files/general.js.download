        getTotalSlides: function() {
            return this.$element.find(this.options.slideElemWords + ' ' + this.options.slideElem).length;
        },

        goToSlide: function(slideNum) {
            var that = this,
                $currentSlideWords = this.$element.find(this.options.slideElemWords + ' ' + this.options.slideElem + '.active'),
                $targetSlideWords = this.$element.find(this.options.slideElemWords + ' ' + this.options.slideElem).eq(slideNum - 1),
                $targetSlideImage = this.$element.find(this.options.slideElemImages + ' ' + this.options.slideElem).eq(slideNum - 1),
                $currentSlideImage = this.$element.find(this.options.slideElemImages + ' ' + this.options.slideElem + '.active');

            //do things with words
            $targetSlideWords.finish().show().animate({
                top: 0
            }, this.options.animationdDurationWords, this.options.easing).addClass('active');

            //get rid of current slide, then reset it
            $currentSlideWords.finish().animate({
                top: '100%'
            }, this.options.animationdDurationWords, this.options.easing).removeClass('active').animate({
                top: '-100%'
            }, 10);

            //do things with images
            $targetSlideImage.finish().fadeIn(this.options.animationdDurationImages).addClass('active');

            //get rid of others
            $currentSlideImage.finish().fadeOut(this.options.animationdDurationImages * 1.25).removeClass('active');
        },

        autoGo: function() {
            var currentSlide = parseInt(this.getCurrentSlide(), 10),
                totalSlides = this.getTotalSlides(),
                nextSlide = (currentSlide + 1);

            if ( nextSlide > totalSlides ) {
                nextSlide = 1;
            }

            this.setActiveNav(nextSlide);
            this.goToSlide(nextSlide);
        }
    };

    $.fn.horsetornado = function(option) {
        var args = Array.prototype.slice.call(arguments, 1);

        if (typeof option === 'string' && $.inArray(option, []) !== -1) {
            var data = this.data('horsetornado');
            return data[option].apply(data, args);
        }

        return this.each(function() {
            var $this = $(this),
               data = $this.data('horsetornado'),
               options = typeof option == 'object' && option;

            if (!data) {
                $this.data('horsetornado', data = new HorseTornado(this, options));
            }

            if (typeof option == 'string') {
                data[option]();
            }
        });
    };

    $.fn.horsetornado.defaults = {
        'animationdDurationWords': 1000,
        'animationdDurationImages': 1000,
        'slideElemWords': '.tornado-words',
        'slideElemImages': '.tornado-images',
        'slideElem': '.slide',
        'navElem': '.tornado-nav',
        'easing': 'easeOutExpo',
        'autoGo': true,
        'waitTime': 10000
    };
}(window.jQuery);

