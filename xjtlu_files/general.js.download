//----------------------------------------------------
//      ResizeListener
//----------------------------------------------------
ResizeListener = (function() {

    var handlers = [];

    var resizeListener = function() {
        this.init();
    };

    resizeListener.prototype.init = function() {
        var resize_handler = createDelegate( this, this.onResize );
        if (window.addEventListener) {
            window.addEventListener('resize', resize_handler, false);
        } else if (window.attachEvent) {
            window.attachEvent('onresize', resize_handler);
        }
    };

    resizeListener.prototype.attachHandler = function( new_handler ) {
        handlers.push( new_handler );
    };

    resizeListener.prototype.onResize = function() {
        //run through handlers and fire a resize event
        for (var i = 0; i < handlers.length; i++) {
            //console.log( ' send resize event to:  ', handlers[i] );
            handlers[i]();
        }
    };

    return resizeListener;

})();

//----------------------------------------------------
//      get query strings
//      from: https://css-tricks.com/snippets/javascript/get-url-variables/
//----------------------------------------------------

function getQueryVariable(variable) {
    var query = window.location.search.substring(1),
        vars = query.split("&");

    for (var i=0;i<vars.length;i++) {
        var pair = vars[i].split("=");

        if(pair[0] == variable){
            return pair[1];
        }
    }

    return(false);
}

