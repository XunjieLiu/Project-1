ImageScrollController = (function() {
    var $window = $( window );
    var $ele;

    var windowHeight;
    var dwh;
    var documentHeight;

    var imageScroll = function() {
        this.init();
    };

    imageScroll.prototype.init = function() {
        documentHeight =  $( document ).height();

        $ele = $( '.image-bg' );

        //do a resize event on init to setup some sizes
        this.resizeHandler();

        var offset = ($ele.offset().top - $ele.height());
        $ele.attr( 'data-offset', offset );
        var height = $ele.height();
        $ele.attr( 'data-height', height );

        //do a scroll event on init to setup some sizes
        this.scrollHandler();

        var scroll_handler = createDelegate( this, this.scrollHandler );
        var resize_handler = createDelegate( this, this.resizeHandler );

        if (window.addEventListener) {
            window.addEventListener('scroll', scroll_handler, false);
            //window.addEventListener('resize', resize_handler, false);
        } else if (window.attachEvent) {
            window.attachEvent('onscroll', scroll_handler);
            //window.attachEvent('onresize', resize_handler);
        }
    };

    imageScroll.prototype.resizeHandler = function() {
        //console.log( 'ImageScrollController: resizeHandler' );
        windowHeight = $window.height();
        dwh = documentHeight - windowHeight;
    };

    imageScroll.prototype.scrollHandler = function() {
        //console.log( 'imageScroll: scrollHandler' );
        if ( displaySize != MOBILE && !OLDIE )  {
            var scroll_top = $window.scrollTop();
            var figure_from_top = (scroll_top - parseInt( $ele.attr( 'data-offset' ) )) - parseInt( $ele.height() );
            var figure_from_bottom = (figure_from_top + windowHeight);

            var pc_up_screen = (figure_from_bottom / (windowHeight + parseInt( $ele.height() )) * 100);

            //console.log( 'imageScroll: scroll_top: ' + scroll_top );
            //console.log( 'imageScroll: figure_from_top: ' + figure_from_top );
            //console.log( 'imageScroll: figure_from_bottom: ' + figure_from_bottom );
            //console.log( 'imageScroll: pc_up_screen: ' + pc_up_screen );

            if ( pc_up_screen > 0 && pc_up_screen < 100 ) {
                $ele.find( 'img' ).css( {
                    top: pc_up_screen + '%',
                    'transform': 'translateY(-' + ( pc_up_screen ) + '%)'
                } );
            }
        }
    };

    return imageScroll;
})();

/*
    Sharer
    Basic URL combinificatortron for facebook/twitter sharer
    Author: Phil Steer, Rckt
*/
sharer = (function () {
    var sharer = { version : "0.1" };

    function twitter( elem, text, url, hashtags, via, related ) {
        var intentLink = 'https://twitter.com/intent/tweet?';

        if ( elem ) {
            if ( elem.attr('data-text').length > 0 ) {
                text = 'text=' + encodeURIComponent( elem.attr('data-text').trim() );
            } else {
                text = 'text=' + encodeURIComponent( $('title').text().trim() );
            }

            if ( elem.attr('data-url') ) {
                url = '&url=' + encodeURIComponent( elem.attr('data-url') );
            } else {
                url = '&url=' + encodeURIComponent( window.location.href );
            }

            if ( elem.attr('data-hashtags') ) {
                hashtags = '&hashtags=' + encodeURIComponent( elem.attr('data-hashtags') );
            } else {
                hashtags = '';
            }

            if ( elem.attr('data-via') ) {
                via = '&via=' + elem.attr('data-via');
            } else {
                via = '';
            }

            if ( elem.attr('data-related') ) {
                related = '&related=' + encodeURIComponent( elem.attr('data-related') );
            } else {
                related = '';
            }
        } else {
            if ( text ) {
                text = 'text=' + encodeURIComponent( text.trim() );
            } else {
                console.log('no text parameter passed - required!');
            }

            if ( url ) {
                url = '&url=' + encodeURIComponent( url );
            } else {
                url = '&url=' + encodeURIComponent( window.location.href + window.location.hash );
            }

            if ( hashtags ) {
                hashtags = '&hashtags=' + encodeURIComponent( hashtags );
            } else {
                hashtags = '';
            }

            if ( via ) {
                via = '&via=' + via;
            } else {
                via = '';
            }

            if ( related ) {
                related = '&related=' + related;
            } else {
                related = '';
            }
        }

        intentLink = intentLink + text + url + hashtags + via + related;
        window.open(intentLink, 'Share', 'width=640,height=400,scrollbars=1,menubar=0,status=0,toolbar=0');
    }

    function facebook() {
        var link = 'http://www.facebook.com/sharer/sharer.php?u=' + encodeURIComponent( window.location.href );
        window.open(link, 'Share', 'width=560,height=400,scrollbars=1,menubar=0,status=0,toolbar=0');
    }

    sharer.twitter = twitter;
    sharer.facebook = facebook;

    return sharer;
})();

