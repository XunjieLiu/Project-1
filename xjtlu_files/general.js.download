//----------------------------------------------------
//      programme filtering
//----------------------------------------------------
ProgrammeFilters = (function() {

    var $PROGRAMMES;

    var departmentFilters = [];
    var levelFilters = [];

    var filters = function() {
        this.init();
    };

    filters.prototype.init = function() {
        $PROGRAMMES = $('#programme-list').isotope({
            itemSelector: 'li',
            layoutMode: 'fitRows',
            fitRows: {
                gutter: 20
            }
        });

        var button_handler = createDelegate( this, this.controlButtonHandler );
        $('.controls button' ).on('click', button_handler );

        //check if we want to pre-filter the page
        //ie - via the query string
        var prefiltersDep = getQueryVariable('dep'),
            prefiltersLev = getQueryVariable('lev');

        //console.log(prefiltersDep, prefiltersLev);

        if ( prefiltersDep.length > 0 ) {
            //console.log('we have prefilterDep');
            this.addProgrammeFilter(prefiltersDep, 'dep');
        }

        if ( prefiltersLev.length > 0 ) {
            //console.log('we have prefilterLev');
            this.addProgrammeFilter(prefiltersLev, 'lev');
        }

        if ( prefiltersDep.length > 0 || prefiltersLev.length > 0 ) {
            var targetOffset = ($('#programmes').offset().top - 140);

            if ( $(window).width() < 768 ) {
                targetOffset = targetOffset - 100;
            }

            console.log(targetOffset);

            $('html, body').animate({
                scrollTop: targetOffset
            }, 1000);
        }
    };

    filters.prototype.controlButtonHandler = function( e ) {

        var target = $( e.currentTarget );
        var filter = target.attr( 'data-filter' );
        var filter_type = target.attr( 'data-type' );

        if ( target.hasClass( 'selected' ) ){
            this.removeProgrammeFilter(filter, filter_type)
        } else {
            this.addProgrammeFilter(filter, filter_type);
        }
    };

    filters.prototype.addProgrammeFilter = function( filter, type ) {
        if ( type == 'dep' ) {
            if (filter == '*') {
                departmentFilters = [];
                $( 'button[data-type="dep"]' ).removeClass( 'selected' );
            } else {
                departmentFilters.push( '.'+filter );
                $( 'button[data-type="dep"][data-filter='+filter+']' ).addClass( 'selected' );
            }
        } else if ( type == 'lev' ) {
            if (filter == '*'){
                levelFilters = [];
                $( 'button[data-type="lev"]' ).removeClass( 'selected' );
            } else {
                levelFilters = [];
                $( 'button[data-type="lev"]' ).removeClass( 'selected' );

                if ( filter == 'othercourses' || filter == 'executive' ) {
                    //no departments should be selected when 'other courses' is clicked
                    departmentFilters = [];
                    $( 'button[data-type="dep"]' ).removeClass( 'selected' );
                }

                levelFilters.push( '.'+filter );
                $( 'button[data-type="lev"][data-filter='+filter+']' ).addClass( 'selected' );
            }
        }

        this.updateList();
    };

    filters.prototype.removeProgrammeFilter = function( filter, type ) {
        var index;

        if ( type == 'dep' ) {
            index = departmentFilters.indexOf( '.' + filter );
            if (index != -1){
                departmentFilters.splice(index, 1);
                $( 'button[data-type="dep"][data-filter='+filter+']' ).removeClass( 'selected' );
            }
        } else if ( type == 'lev' ) {
            index = levelFilters.indexOf( '.' + filter );
            if (index != -1){
                levelFilters.splice(index, 1);
                $( 'button[data-type="lev"][data-filter='+filter+']' ).removeClass( 'selected' );
            }
        }

        this.updateList();
    };

    filters.prototype.updateList = function() {
        var filters = [];
        //for each dept filter, add a level filer to it
        if ( (departmentFilters.length) && !(levelFilters.length) ){
            filters = departmentFilters;
        }
        if ( (levelFilters.length) && !(departmentFilters.length) ){
            filters = levelFilters;
        }
        if ( (levelFilters.length) && (departmentFilters.length) ){
            for ( var i = 0; i < departmentFilters.length; i++ ) {
                for ( var j = 0; j < levelFilters.length; j++ ) {
                    filters.push( departmentFilters[i].toString() + levelFilters[j].toString() );
                }
            }
        }
        if ( !(levelFilters.length) && !(departmentFilters.length) ){
            filters.push('*');
        }

        //force adding short courses back in, no matter what faculties are selected
        if (levelFilters.indexOf( '.ot' ) != -1 && departmentFilters.length){
            filters.push('.ot');
        }

        //console.log( "filters: ", filters );

        $PROGRAMMES.isotope({ filter: filters.toString() });
    };

    return filters;
})();

