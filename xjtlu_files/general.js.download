if ( $('.programme-search').length > 0 ) {
    var $searchProgrammes = $('#searchprogrammes'),
        $typeAhead = $('#typeahead');

    function filterTypeahead(val) {
        if (val == '') {
            $typeAhead.hide();
            return;
        }

        var $shown = $typeAhead.find('li').hide().filter(function() {
            var tokens = $(this).data('title').split(',');
            for (var i = 0; i < tokens.length; i++) {
                var token = tokens[i].toLowerCase();
                if (token.indexOf(val) !== -1) {
                    return true;
                }
            }

            return false;
        }).show();

        $typeAhead.toggle($shown.length > 0);
    }

    $searchProgrammes.on('input', function(e) {
        var val = $searchProgrammes.val().toLowerCase();
        filterTypeahead(val);
    }).each(function() {
        var val = $searchProgrammes.val().toLowerCase();
        if (val == '') {
            return;
        }

        filterTypeahead(val);
        $searchProgrammes.focus();
    });

    $('#programme-search').on('submit', function(e) {
        e.preventDefault();

        if (!$typeAhead.is(':visible')) {
            return;
        }

        var link = $typeAhead.find('li:visible a').first().attr('href');
        window.location.href = link;
    });
}

