//----------------------------------------------------
//      stuff after dom load
//----------------------------------------------------

var $MAIN_NAV = $( '#main-nav' ),
    $SUB_NAV = $( '#sub-nav' ),
    MOBILE = 'mobile',
    DESKTOP = 'desktop',
    displaySize = '';
    OLDIE =  $('html.no-borderradius').length !== 0;

function getDisplaySize() {
    var display = '';

    if ( $('.size.mobile').css('display') == "block" ) {
        display = MOBILE;
    }

    if ( $('.size.desktop').css('display') == "block" ) {
        display = DESKTOP;
    }

    return display;
}

function changeDisplaySize( size ) {
    if ( size == MOBILE ) {
        this.enableMobile();
    } else {
        this.disableMobile();
    }

    displaySize = size;
}

function resizer() {
    if ( displaySize != getDisplaySize() ){
        changeDisplaySize( getDisplaySize() );
    }
}

function enableMobile() {
    $MAIN_NAV.mainmenu().sticky( 'unstick' );
    $SUB_NAV.sticky( 'unstick' ).sticky( {topSpacing: 0, widthFromWrapper: true, responsiveWidth:true } );
}

function disableMobile() {
     $MAIN_NAV.mainmenu().sticky( { widthFromWrapper: true, responsiveWidth: true } );
     $SUB_NAV.sticky( 'unstick' ).sticky( {topSpacing: 70, widthFromWrapper: true, responsiveWidth:true } );
}

$(document).ready(function() {
    //----------------------------------------------------
    //      everything else
    //----------------------------------------------------
    var resize_listener = new ResizeListener();

    if ( $( '#programme-list' ).length > 0 && !OLDIE ) {
        var filters = new ProgrammeFilters();
    }

    if ( $( '.accordion-section' ).length > 0 ) {
        var accordion_controller = new AccordionController();
    }

    if ( $SUB_NAV.length > 0 ) {
        var waypoint_controller = new WaypointController();
    }

    if ( $('.horse-tornado').length > 0 ) {
        $('.horse-tornado').horsetornado();
    }

    if ( $( '.image-bg' ).length > 0 && !OLDIE ) {
        var image_scroller = new ImageScrollController();
        resize_listener.attachHandler( image_scroller.resizeHandler );
    }

    if ( $('.gallery-block').length > 0 ) {
        $('.gallery-block .lightbox').venobox({
            titleattr: 'data-caption'
        });

        var gallery_controller = new GalleryController();
        resize_listener.attachHandler( createDelegate( gallery_controller, gallery_controller.resizeHandler ) );
    }

    if ( $('.news-article .video-block.helix').length > 0 ) {
        var video_controller = new VideoController();
        resize_listener.attachHandler( createDelegate( video_controller, video_controller.resizeHandler ) );
    }

    if ( $('.events .events-list').length > 0 ) {
        $('.events-list').combineevents();
    }

    resizer();
    resize_listener.attachHandler( resizer );

    if ( $('.gallery-block .hideme').length > 0 ) {
        $('.gallery-block').hide();
    }
});


