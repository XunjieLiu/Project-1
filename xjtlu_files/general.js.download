/**
 * MainMenu
 *
 * @author Phil Steer <phil.steer@rckt.co.uk>
 * @package XJTLU
 */
!function($) {
    var MainMenu = function(elem, opts) {
        this.init('mainmenu', elem, opts);
    };

    MainMenu.prototype = {
        constructor: MainMenu,
        init: function(type, elem, opts) {
            this.type = type;
            this.$element = $(elem);
            this.options = this.getOptions(opts);
            this.setup();
        },

        getOptions: function(opts) {
            return $.extend({}, $.fn[this.type].defaults, this.$element.data(), opts);
        },

        setup: function() {
            var that = this;

            //----------------------------------------------------
            //      top level nav
            //----------------------------------------------------

            this.$element.on('click', this.options.topLevelClass + ' a', function(e) {
                e.preventDefault();

                //----------------------------------------------------
                //      desktop
                //----------------------------------------------------
                if ( $(window).width() > 767 ) {
                    //console.log('desktop');
                    //find stuff
                    var targetSection = $(this).attr('data-section'),
                        currentSection = $('.nav-section:visible').attr('data-for'),
                        $targetSectionElem = that.$element.find(that.options.subLevelClass + ' > div[data-for="' + targetSection + '"]'),
                        subNavIsOpen = that.$element.find(that.options.subLevelClass).is(':visible'),
                        $targetTopNavElem = $(that.options.topLevelClass + ' a[data-section="' + targetSection + '"]'),
                        baseHref = $(document).find('base').attr('href');

                    //console.log(baseHref);

                    //if we have a sub nav for the section
                    if ( $targetSectionElem.length != 0 ) {
                        //if it's already open
                        if ( subNavIsOpen ) {
                            //it's open and we clicked the same section again
                            if ( currentSection == targetSection ) {
                                //console.log('already open, so just close');
                                //we just close the menu and hide the subnav container
                                that.$element.find(that.options.subLevelClass).slideUp(250);
                                $targetSectionElem.fadeOut(250);
                                $targetTopNavElem.removeClass('open');
                            } else {
                                //console.log('hide, then show again');
                                //we hide subnav, find the right new subnav container and then show the menu again
                                that.$element.find(that.options.subLevelClass).slideUp(250).delay(250).slideDown(250);

                                window.setTimeout(function() {
                                    $('.nav-section').fadeOut(125);
                                    $targetSectionElem.fadeIn(350);
                                }, 250);

                                $targetTopNavElem.closest('li').siblings().find('a').removeClass('open');
                                $targetTopNavElem.addClass('open');
                            }
                        } else {
                            //console.log('just show, as nothing already open');
                            //go find the subnav we want and show it
                            $targetSectionElem.show();
                            that.$element.find(that.options.subLevelClass).slideDown(250);
                            $targetTopNavElem.addClass('open');
                        }
                    } else {
                        //top level has no subnav - it's probably news
                        //console.log('we should go somewhere, no subnav here');
                        if ( $(this).attr('href') == baseHref ) {
                            window.location.href = $(this).attr('href');
                        } else {
                            window.location.href = baseHref + $(this).attr('href');
                        }

                    }
                }
            });

            //----------------------------------------------------
            //      subnav
            //----------------------------------------------------

            this.$element.on('click', this.options.subLevelClass + ' .nav-section-heading', function(e) {
                if ( $(window).width() < 768 ) {
                    e.preventDefault();

                    var $currentSection = that.$element.find('.nav-section-content:visible'),
                        currentSectionName = $currentSection.parent('.nav-section').attr('data-for'),
                        $targetSection = $(this).siblings('.nav-section-content'),
                        targetSectionName = $(this).parent('.nav-section').attr('data-for');

                    //console.log('currentSection: ', $currentSection, 'targetSection: ', $targetSection, 'currentSectionName: ', currentSectionName, 'targetSectionName: ', targetSectionName);

                    if ( !currentSectionName ) {
                        //console.log('nothing open, so open');
                        $targetSection.slideDown(250);
                    }

                    if ( currentSectionName && currentSectionName != targetSectionName ) {
                        //console.log('something open, not this one');
                        $currentSection.slideUp(250);
                        $targetSection.slideDown(250);
                    }

                    if ( currentSectionName && currentSectionName == targetSectionName ) {
                        //console.log('something open, this one');
                        $currentSection.slideUp(250);
                    }
                }
            });
        }
    };

    $.fn.mainmenu = function(option) {
        var args = Array.prototype.slice.call(arguments, 1);

        if (typeof option === 'string' && $.inArray(option, []) !== -1) {
            var data = this.data('mainmenu');
            return data[option].apply(data, args);
        }

        return this.each(function() {
            var $this = $(this),
               data = $this.data('mainmenu'),
               options = typeof option == 'object' && option;

            if (!data) {
                $this.data('mainmenu', data = new MainMenu(this, options));
            }

            if (typeof option == 'string') {
                data[option]();
            }
        });
    };

    $.fn.mainmenu.defaults = {
        'topLevelClass': '.nav-top-level',
        'subLevelClass': '.nav-sub-level'
    };
}(window.jQuery);

/**
 * HorseTornado
 *
 * @author Phil Steer <phil.steer@rckt.co.uk>
 * @package XJTLU
 */
!function($) {
    var HorseTornado = function(elem, opts) {
        this.init('horsetornado', elem, opts);
    };

    HorseTornado.prototype = {
        constructor: HorseTornado,
        init: function(type, elem, opts) {
            this.type = type;
            this.$element = $(elem);
            this.options = this.getOptions(opts);
            this.setup();
        },

        getOptions: function(opts) {
            return $.extend({}, $.fn[this.type].defaults, this.$element.data(), opts);
        },

        setup: function() {
            var that = this;

            //generate nav and make first button active
            this.generateNav();
            this.$element.find(this.options.navElem + ' button').eq(0).addClass('active');

            //set all but first words slides aria-hidden attr
            this.$element.find(this.options.slideElemWords + ' ' + this.options.slideElem).each(function(index) {
                if ( index == 0 ) {
                    $(this).attr('aria-hidden', false).css({
                        top: 0,
                        display: 'block'
                    }).addClass('active');
                } else {
                    $(this).attr('aria-hidden', true).css({
                        top: '-100%',
                        display: 'none'
                    });
                }
            });

            //
            this.$element.find(this.options.slideElemImages + ' ' + this.options.slideElem).eq(0).addClass('active').show();

            if ( this.options.autoGo === true ) {
                //set up the auto slider
                var autoGoTimer = setInterval(function() {
                    that.autoGo()
                }, parseInt(this.options.waitTime,10));
            }

            //nav button click events
            this.$element.on('click', this.options.navElem + ' button', function() {
                if ($(this).hasClass('active')) {
                    return false;
                }

                //interrupt the auto slider
                if (that.options.autoGo === true) {
                    clearInterval(autoGoTimer);
                }

                var slideNum = parseInt($(this).attr('data-slide'), 10);
                that.setActiveNav(slideNum);
                that.goToSlide(slideNum);

                //set wait timer going again
                if (that.options.autoGo === true) {
                    autoGoTimer = setInterval( function() {
                        that.autoGo()
                    }, parseInt(that.options.waitTime,10));
                }
            });
        },

        generateNav: function() {
            var that = this,
                buttonHtml = '';

            $(this.options.slideElemWords + ' ' + this.options.slideElem).each(function() {
                buttonHtml = buttonHtml + '<button type="button" data-slide="' + ($(this).index() + 1) + '">Show slide ' + ( $(this).index() + 1 ) + '</button>';
            });

            this.$element.find(this.options.slideElemWords).append('<nav class="' + this.options.navElem.replace('.','') + '">' + buttonHtml + '</nav>');
        },

        setActiveNav: function(slideNum) {
            this.$element.find(this.options.navElem + ' button[data-slide="' + slideNum + '"]').addClass('active').siblings('button').removeClass('active');
        },

        getCurrentSlide: function() {
            return this.$element.find(this.options.slideElemWords + ' ' + this.options.slideElem + '.active').attr('data-slide');
